                                            <option value="Neurology">Neurology</option>
                                            <option value="Opthalmology">Opthalmology</option>
                                            <option value="Orthopedics">Orthopedics</option>
                                            <option value="Cancer Departmen">Cancer Department</option>
                                            <option value="ENT Departmen">ENT Department</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        
                                        <input type="text" name="d_name"  placeholder="Doctor name" id="">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        
                                        <div class="cal-icon">
                                            <input type="text" class="form-control datetimepicker" placeholder="Date" name="date">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <div class="time-icon">
                                            <input type="text" class="form-control" id="datetimepicker3" name="time" placeholder="Time">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        
                                        <input class="form-control" type="email" name="email" placeholder="Patient Email">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                      
                                        <input class="form-control" type="text" name="phone" placeholder="Patient Phone Number">
                                    </div>
                                </div>
                            </div>
                            
                            
                            <div class="m-t-20 text-center">
                                <button class="btn btn-primary submit-btn">Create Appointment</button>
                            </div>
