                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
										
										<input class="form-control" type="text" placeholder="Patient Name" name="p_name" >
									</div>
                                </div>
                               
                            </div>
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
